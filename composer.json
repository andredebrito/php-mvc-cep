        "ext-json": "*",
        "ext-curl": "*",
        "andredebrito/phpviacep": "1.0.5",
        "league/plates": "v4.0.0-alpha",
        "coffeecode/optimizer": "^2.0",
        "coffeecode/router": "^1.0"
